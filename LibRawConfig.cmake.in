include(CMakeFindDependencyMacro)
# Capturing values from configure (optional)
set(my-config-var @my-config-var@)

# Same syntax as find_package
find_dependency(OpenMP)
find_dependency(ZLIB)
find_dependency(JPEG)

# Any extra setup

# Add the targets file
include( "${CMAKE_CURRENT_LIST_DIR}/LibRawTargets.cmake" )

# Create an alias libraw::libraw. Because of libraw::raw is imported,
# an interface library with dependency on it is created and the alias is to that library.
add_library(libraw_raw INTERFACE)
target_link_libraries(libraw_raw INTERFACE libraw::raw)
add_library(libraw::libraw ALIAS libraw_raw)